project Alsada is

   for Object_Dir use "obj";

   package Compiler is
      for Switches ("ada") use ("-gnatVars", "-gnatwa", "-gnata", "-g", "-gnat12");
   end Compiler;

   package Binder is
      for Switches ("ada") use ("-E");
   end Binder;

   for Source_Dirs use ("src", "src/test");
   for Main use ("main.adb");
end Alsada;

